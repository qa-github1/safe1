    "tests-org2": "npm run clearReport && cypress run --headless --browser chrome --spec 'src/specs/org1/*spec.js' --env environment=pentest,orgNum=2",
    "tests-org3": "npm run clearReport && cypress run --headless --browser chrome --spec 'src/specs/org1/*spec.js' --env environment=pentest,orgNum=3",
    "tests-org4": "npm run clearReport && cypress run --headless --browser chrome --spec 'src/specs/org1/*spec.js' --env environment=pentest,orgNum=4",
