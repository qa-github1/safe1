    "docker-group3": "./node_modules/.bin/cypress run --browser chrome --spec 'src/specs/org3/*specc.js'",
    "docker-group4": "./node_modules/.bin/cypress run --browser chrome --spec 'src/specs/org4/*specc.js'",
